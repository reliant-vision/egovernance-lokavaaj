{
    "AWSEBDockerrunVersion": "1",
    "containerDefinitions": [
      {
        "name": "backend",
        "image": "backend",
        "essential": true,
        "memory": 512,
        "ports": [
          {
            "hostPort": 0,
            "containerPort": 5000
          }
        ]
      },
      {
        "name": "nginx",
        "image": "nginx:1.21",
        "essential": true,
        "memory": 512,
        "links": ["backend"],
        "ports": [
          {
            "hostPort": 0,
            "containerPort": 80
          }
        ]
      }
    ],
    "ports": 80
  }
  