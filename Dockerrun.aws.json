{
    "AWSEBDockerrunVersion": "2",
    "containerDefinitions": [
      {
        "name": "backend",
        "image": {"name": "backend", "update": true},
        "essential": true,
        "memory": 512,
        "portMappings": [
          {
            "hostPort": 0,
            "containerPort": 5000
          }
        ]
      },
      {
        "name": "nginx",
        "image": "nginx:1.21",
        "essential": true,
        "memory": 512,
        "portMappings": [
          {
            "hostPort": 0,
            "containerPort": 80
          }
        ]
      }
    ]
  }
  