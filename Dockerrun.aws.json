{
    "AWSEBDockerrunVersion": "1",
    "containerDefinitions": [
        {
            "name": "backend",
            "essential": true,
            "memory": 512,
            "portMappings": [
                {
                    "hostPort": 80,
                    "containerPort": 5000
                }
            ],
            "environment": [
                {
                    "name": "PORT",
                    "value": "5000"
                }
            ]
        },
        {
            "name": "nginx",
            "essential": true,
            "memory": 512,
            "links": ["backend"],
            "portMappings": [
                {
                    "hostPort": 80,
                    "containerPort": 80
                }
            ],
            "environment": [
                {
                    "name": "PORT",
                    "value": "80"
                }
            ]
        }
    ]
}
